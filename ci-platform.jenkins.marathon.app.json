{
  "id": "/ci-platform/jenkins",
  "cmd": null,
  "cpus": 0.01,
  "mem": 1500,
  "disk": 0,
  "instances": 1,
  "constraints": [
    [
      "hostname",
      "LIKE",
      "toaster.tinc-core-vpn"
    ]
  ],
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/config",
        "hostPath": "/data/sync-service/folders/ci-platform/config-volume",
        "mode": "RW"
      },
      {
        "containerPath": "/var/lib/jenkins",
        "hostPath": "/data/sync-service/folders/ci-platform/jenkins",
        "mode": "RW"
      },
      {
        "containerPath": "/var/log",
        "hostPath": "/data/sync-service/folders/ci-platform/jenkins-logs",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "azulinho/ci-platform",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 31002,
          "hostPort": 31002,
          "servicePort": 10007,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 31003,
          "hostPort": 31003,
          "servicePort": 10006,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 9000,
          "hostPort": 31004,
          "servicePort": 10015,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "JAVA_OPTS": " -Djava.awt.headless=true ",
    "JENKINS_MESOS_LIBPROCESS_ADVERTISE_IP": "169.254.100.19",
    "JENKINS_HOME": "/var/lib/jenkins",
    "JENKINS_MESOS_LIBPROCESS_ADVERTISE_PORT": "31004",
    "TEMPURL": "http://tempurl-endpoint.service.tinc-core-vpn/api?tempurl=jenkins_config_yml_local"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 600,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "path": "/login",
      "protocol": "HTTP",
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "HAPROXY_2_GROUP": "external",
    "HAPROXY_0_MODE": "http",
    "HAPROXY_1_PORT": "31003",
    "HAPROXY_2_PORT": "31004",
    "HAPROXY_1_MODE": "tcp",
    "HAPROXY_0_PORT": "31002",
    "HAPROXY_0_GROUP": "external",
    "HAPROXY_1_GROUP": "external",
    "HAPROXY_2_MODE": "tcp",
    "HAPROXY_0_VHOST": "jenkins.service.tinc-core-vpn"
  },
  "portDefinitions": [
    {
      "port": 10007,
      "protocol": "tcp",
      "name": "default",
      "labels": {}
    },
    {
      "port": 10006,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10015,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
